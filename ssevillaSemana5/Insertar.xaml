<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ssevillaSemana5.Insertar">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="insertar estudiante" FontSize="20" HorizontalTextAlignment="Center"/>
            <Entry x:Name="txtCodigo" Placeholder="ingresar el codigo" Keyboard="Numeric"/>
            <Entry x:Name="txtNombre" Placeholder="ingresar el nombre"/>
            <Entry x:Name="txtApellido" Placeholder="ingresar el Apellido"/>
            <Entry x:Name="txtEdad" Placeholder="ingresar la edad"/>
            <Button x:Name="btnIngresar" Text="ingresar" Clicked="btnIngresar_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>
    